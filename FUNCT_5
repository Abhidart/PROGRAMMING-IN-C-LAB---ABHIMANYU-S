// program to find the sum of 2 numbers - with argument without return value
#include<stdio.h>

void getSum(int, int);

int main() {
    int num1, num2;
    printf("Enter the two numbers : ");
    scanf("%d%d",&num1,&num2);
    getSum(num1, num2);
}

void getSum(int num1, int num2) {
    int sum; 
    sum = num1 + num2;
    printf("%d + %d = %d",num1, num2, sum);
}
