#include<stdio.h>
void swap(int *,int *); 
void main( )
{
    int a = 10, b = 20;
    printf("Before swapping\n");
    printf ("a=%d b=%d\n",a,b);
    swap(&a,&b);
    printf("After swapping\n");
    printf("a=%d b=%d",a,b);
    return 0;
    }
    void swap(int *x,int *y)	
    {
    int t;
    t=*x;
    *x=*y;
    *y=t;	
    }
